{
    "_version": "1.12.0",
    "sap.app": {
        "id": "nmshd.app",
        "type": "application",
        "i18n": "i18n/i18n.properties",
        "title": "{{appTitle}}",
        "description": "{{appDescription}}",
        "applicationVersion": {
            "version": "2.0.0-beta.19"
        },
        "dataSources": {}
    },
    "sap.ui": {
        "technology": "UI5",
        "deviceTypes": {
            "desktop": true,
            "tablet": true,
            "phone": true
        }
    },
    "sap.ui5": {
        "rootView": {
            "viewName": "nmshd.app.shell.App",
            "type": "XML",
            "async": true,
            "id": "app"
        },
        "dependencies": {
            "minUI5Version": "1.84.0",
            "libs": {
                "sap.ui.core": {},
                "sap.m": {},
                "sap.ui.layout": {},
                "sap.ui.unified": {}
            }
        },
        "models": {
            "t": {
                "type": "sap.ui.model.resource.ResourceModel",
                "settings": {
                    "bundleName": "nmshd.app.i18n.i18n"
                }
            },
            "Device": {
                "type": "sap.ui.model.json.JSONModel"
            }
        },
        "resources": {},
        "routing": {
            "config": {
                "routerClass": "sap.f.routing.Router",
                "viewType": "XML",
                "viewPath": "nmshd.app.shell",
                "controlId": "app",
                "controlAggregation": "pages",
                "async": true
            },
            "routes": [
                {
                    "pattern": "",
                    "name": "start",
                    "target": "generic"
                },
                {
                    "pattern": "app//:viewPattern:",
                    "name": "generic",
                    "target": [
                        {
                            "name": "generic",
                            "prefix": "generic"
                        }
                    ],
                    "greedy": true
                },
                {
                    "pattern": "app/:viewPattern:",
                    "name": "generic",
                    "target": [
                        {
                            "name": "generic",
                            "prefix": "generic"
                        }
                    ],
                    "greedy": true
                },
                {
                    "pattern": "app/:all*:",
                    "name": "generic2",
                    "target": [
                        {
                            "name": "generic",
                            "prefix": "generic"
                        }
                    ]
                }
            ],
            "targets": {
                "start": {
                    "viewName": "Start",
                    "viewLevel": 1,
                    "viewId": "idStart"
                },
                "generic": {
                    "viewName": "Generic",
                    "viewLevel": 1,
                    "viewId": "generic"
                }
            }
        }
    }
}
