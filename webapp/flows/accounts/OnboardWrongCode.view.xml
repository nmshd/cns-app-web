<mvc:View controllerName="nmshd.app.flows.accounts.OnboardWrongCode"
	xmlns="sap.m"
	xmlns:m="sap.m"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns:semantic="sap.m.semantic"
	xmlns:form="sap.ui.layout.form"
	xmlns:l="sap.ui.layout"
	xmlns:controls="nmshd.app.core.controls">
	<semantic:DetailPage id="wizardContentPage"
		navButtonPress="onNavButtonPress"
		showNavButton="true"
		title="{t>accounts.create.title}"
		showFooter="false"
		busy="{v>/busy}"
		busyIndicatorDelay="{v>/delay}">


		<semantic:customHeaderContent>

		</semantic:customHeaderContent>

		<semantic:subHeader>


		</semantic:subHeader>

		<semantic:content>
			<m:Title text="Dieser Code ist fehlerhaft :("
				class="sapUiSmallMargin" />

			<controls:Container class="sapUiSmallMargin">


				<m:Text text="Es tut uns leid, aber dieser Code scheint leider fehlerhaft zu sein."
					class="inlineText" />
				<m:Text text=" Vielleicht ist er schon abgelaufen? Teilweise sind die Codes nur eine begrenzte Zeit gültig, um Deine Daten bestmöglich zu schützen."
					class="inlineText" />
				<m:Text text=" Vesuche bitte, bei unserem Partner einen neuen Code erzeugen zu lassen und probiere es erneut?"
					class="inlineText" />

			</controls:Container>
		</semantic:content>
	</semantic:DetailPage>
</mvc:View>
