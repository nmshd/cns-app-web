                    "pattern": "accounts/{accountId}/relationships/{relationshipId}/master",
                    "name": "account.relationship.master",
                    "target": ["account.relationship.master"],
                    "parent": "nmshd.app.Component:phone",
                    "showMaster": true,
                    "detailTarget": "account.relationship.home"
