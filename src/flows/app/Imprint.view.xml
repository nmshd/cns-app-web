<mvc:View
    controllerName="nmshd.app.flows.app.Imprint"
    xmlns:m="sap.m"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns:semantic="sap.m.semantic"
    xmlns:form="sap.ui.layout.form"
>

	<semantic:DetailPage
        id="page"
        navButtonPress="onNavButtonPress"
        showNavButton="{d>/system/phone}"
        title="{t>imprint.title}"
        showFooter="false"
        busy="{v>/busy}"
        busyIndicatorDelay="{v>/delay}"
    >


		<semantic:customHeaderContent>

		</semantic:customHeaderContent>

		<semantic:content>
			<m:VBox>
				<form:SimpleForm>
					<form:content>
						<m:Label text="{t>imprint.text1}" />
						<m:VBox>
							<m:Text text="{t>imprint.text2}" />
							<m:Title text="{t>imprint.text3}" />
							<m:Text text="{t>imprint.text4}" />
							<m:Text text="{t>imprint.text5}" />
							<m:Text text="{t>imprint.text6}" />
						</m:VBox>
						<m:Label text="{t>imprint.text7}" />
						<m:Text text="{t>imprint.text8}" />
						<m:Label text="{t>imprint.text9}" />
						<m:Text text="{t>imprint.text10}" />
						<m:Label text="{t>imprint.text11}" />
						<m:Text text="{t>imprint.text12}" />



					</form:content>
				</form:SimpleForm>
			</m:VBox>

		</semantic:content>
	</semantic:DetailPage>
</mvc:View>
