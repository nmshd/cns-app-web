<mvc:View
    controllerName="nmshd.app.flows.accounts.OnboardWrongCode"
    xmlns="sap.m"
    xmlns:m="sap.m"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns:semantic="sap.m.semantic"
    xmlns:controls="nmshd.app.core.controls"
>
	<semantic:DetailPage
        id="wizardContentPage"
        navButtonPress="onNavButtonPress"
        showNavButton="true"
        title="{t>accounts.create.title}"
        showFooter="false"
        busy="{v>/busy}"
        busyIndicatorDelay="{v>/delay}"
    >


		<semantic:customHeaderContent>

		</semantic:customHeaderContent>

		<semantic:subHeader>


		</semantic:subHeader>

		<semantic:content>
			<m:Title text="{t>onboardWrongCode.title}" class="sapUiSmallMargin" />

			<controls:Container class="sapUiSmallMargin">


				<m:Text text="{t>onboardWrongCode.text1}" class="inlineText" />
				<m:Text text="{t>onboardWrongCode.text2}" class="inlineText" />
				<m:Text text="{t>onboardWrongCode.text3}" class="inlineText" />

			</controls:Container>
		</semantic:content>
	</semantic:DetailPage>
</mvc:View>
