    "scripts": {
        "lint": "npm run lint:eslint && npm run lint:prettier",
        "lint:eslint": "eslint --ext .js ./src",
        "lint:prettier": "prettier --check .",
        "start": "npx ui5 serve",
        "build": "ui5 build preload --exclude-task createDebugFiles generateVersionInfo replaceCopyright -a",
