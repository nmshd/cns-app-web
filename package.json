        "@wdio/local-runner": "^8.16.4",
        "@wdio/mocha-framework": "^8.16.3",
        "@wdio/spec-reporter": "^8.16.3",
        "babel-preset-transform-ui5": "^7.2.5",
        "chromedriver": "^116.0.0",
        "cross-env": "^7.0.3",
        "eslint": "^8.48.0",
        "eslint-config-prettier": "^9.0.0",
        "express-http-proxy": "^1.6.3",
        "npm-run-all": "^4.1.5",
        "prettier": "^3.0.3",
        "prompt": "^1.3.0",
