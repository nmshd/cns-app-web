        "@nmshd/app-runtime": "2.3.9",
        "@nmshd/content": "^2.6.0",
        "@nmshd/crypto": "^2.0.4",
        "@nmshd/runtime": "2.7.1",
        "@nmshd/transport": "^2.1.0",
        "bson": "^5.4.0",
        "lodash": "^4.17.21",
        "pako": "^2.1.0"
    },
    "devDependencies": {
        "@babel/cli": "^7.22.15",
        "@babel/core": "^7.22.15",
        "@babel/preset-env": "^7.22.15",
        "@babel/preset-typescript": "^7.22.15",
